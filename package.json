    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-csp-header": "^5.1.0",
    "express-openid-connect": "^2.9.0",
    "moment": "^2.29.4",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "concurrently": "^7.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "root": true,
